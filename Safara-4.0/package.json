{
  "name": "safetrailmobile",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@safetrail/frontend & npm run dev --workspace=@safetrail/backend & npm run dev --workspace=@safetrail/authority",
    "build": "npm run build --workspace=@safetrail/frontend && npm run build --workspace=@safetrail/backend && npm run build --workspace=@safetrail/authority",
    "start": "npm run start --workspace=@safetrail/backend",
    "check": "npm run check --workspace=@safetrail/backend"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "@types/pdf-parse": "^1.1.5",
    "baseline-browser-mapping": "^2.8.29",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@google/genai": "^1.31.0",
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "node-fetch": "^3.3.2",
    "qrcode.react": "^4.2.0",
    "sonner": "^2.0.7"
  }
}
